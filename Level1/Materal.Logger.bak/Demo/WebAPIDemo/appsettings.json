{
  "MateralLogger": {
    "Targets": [
      {
        "Enable": true,
        "Name": "ConsoleLog",
        "Type": "Console",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}",
        "Colors": {
          "Trace": "DarkGray",
          "Debug": "DarkGreen",
          "Information": "Gray",
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "Red"
        }
      }
    ],
    "Rules": [
      {
        "Enable": true,
        "Name": "规则1",
        "Targets": [
          "ConsoleLog"
        ],
        "MinLevel": "Trace",
        "MaxLevel": "Critical",
        "LogLevel": {
          "Default": "Trace",
          "Microsoft": "Information",
          "Microsoft.AspNetCore":  "Warning"
        }
      }
    ]
  }
}