{
  //LogLevel:Trace,Debug,Information,Warning,Error,Critical
  //Format: DateTime,Date,Time,Year,Month,Day,Hour,Minute,Second,Level,Scope,Message,CategoryName,Exception,ProgressID,ThreadID,Application,MachineName
  //Path: Date,Year,Month,Day,Hour,Minute,Second,Level,Scope,CategoryName,Exception,ProgressID,ThreadID,Application,MachineName
  "MateralLogger": {
    "Application": "${ApplicationName}",
    "Mode": "Strict", //Normal,Strict
    "LogLogLevel": {
      "MinLevel": "Trace",
      "MaxLevel": "Critical"
    },
    "LogLevel": {
      "Default": "Trace"
    },
    "Targets": [
      {
        "Enable": true,
        "Name": "LifeConsole",
        "Type": "Console",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}",
        "Colors": {
          "Trace": "DarkGray",
          "Debug": "DarkGreen",
          "Information": "Gray",
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "Red"
        }
      },
      {
        "Enable": true,
        "Name": "LevelFile",
        "Type": "File",
        "Path": "${RootPath}\\Logs\\${Date}\\${Level}.log",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}"
      },
      {
        "Enable": true,
        "Name": "HttpLog",
        "Type": "Http",
        "Url": "http://localhost:5000/api/Log/WriteLog",
        "HttpMethod": "POST",
        "Format": "{\"CreateTime\":\"${DateTime}\",\"Application\":\"${Application}\",\"Level\":\"${Level}\",\"Scope\":\"${Scope}\",\"CategoryName\":\"${CategoryName}\",\"MachineName\":\"${MachineName}\",\"ProgressID\":\"${ProgressID}\",\"ThreadID\":\"${ThreadID}\",\"Message\":\"${Message}\",\"Exception\":\"${Exception}\"}"
      },
      {
        "Enable": true,
        "Name": "LocalDB",
        "Type": "Sqlite",
        "Path": "${RootPath}\\Logs\\MateralLogger.db"
        //"ConnectionString": "Data Source=${RootPath}\\Logs\\MateralLogger.db"
      },
      {
        "Enable": true,
        "Name": "ServerDB",
        "Type": "SqlServer",
        "ConnectionString": "${LogDBConnectionString}"
      },
      {
        "Enable": true,
        "Name": "LocalWebSocket",
        "Type": "WebSocket",
        "Port": 5002,
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}",
        "Colors": {
          "Trace": "DarkGray",
          "Debug": "DarkGreen",
          "Information": "Gray",
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "Red"
        }
      }
    ],
    "Rules": [
      {
        "Enable": true,
        "MinLevel": "Trace",
        "MaxLevel": "Critical",
        "Targets": [
          "LifeConsole"
          //"LevelFile",
          //"HttpLog"
          //"LocalDB",
          //"ServerDB"
          //"LocalWebSocket"
        ]
      }
    ]
  }
}