{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning"
    },
    "MateralLogger": {
      "ApplicationName": "MateralLogger",
      "MinLogLevel": "Trace",
      "MaxLogLevel": "Critical",
      "MinLoggerLogLevel": "Trace",
      "MaxLoggerLogLevel": "Critical",
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Warning"
      },
      "Targets": [
        {
          "Enable": true,
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}",
          "Colors": {
            "Trace": "DarkGray",
            "Debug": "DarkGreen",
            "Information": "Gray",
            "Warning": "DarkYellow",
            "Error": "DarkRed",
            "Critical": "Red"
          }
        },
        {
          "Enable": false,
          "Name": "QueueConsoleLog",
          "Type": "QueueConsole",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}",
          "Colors": {
            "Trace": "DarkGray",
            "Debug": "DarkGreen",
            "Information": "Gray",
            "Warning": "DarkYellow",
            "Error": "DarkRed",
            "Critical": "Red"
          }
        },
        {
          "Enable": false,
          "Name": "BatchConsoleLog",
          "Type": "BatchConsole",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}",
          "Colors": {
            "Trace": "DarkGray",
            "Debug": "DarkGreen",
            "Information": "Gray",
            "Warning": "DarkYellow",
            "Error": "DarkRed",
            "Critical": "Red"
          },
          "Batch": {
            "BatchSize": 2000,
            "PushInterval": 2
          }
        }
      ],
      "Rules": [
        {
          "Enable": true,
          "Name": "规则1",
          "MinLevel": "Trace",
          "MaxLevel": "Critical",
          "Targets": [
            "ConsoleLog",
            "QueueConsoleLog",
            "BatchConsoleLog"
          ],
          "LogLevel": {
            "Default": "Trace",
            "Microsoft": "Warning"
          },
          "Scopes": [
            "PublicScope"
          ]
        }
      ]
    }
  }
}
