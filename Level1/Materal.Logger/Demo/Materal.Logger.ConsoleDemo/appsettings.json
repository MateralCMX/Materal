{
  "Logging": {
    //"LogLevel": { //日志等级过滤
    //  "Default": "Trace",
    //  "Microsoft": "Warning"
    //},
    "MateralLogger": {
      "Application": "MateralLogger", //应用程序名称
      "MinLogLevel": "Trace", //最小日志等级
      "MaxLogLevel": "Critical", //最大日志等级
      "MinLoggerLogLevel": "Trace", //最小自身日志等级
      "MaxLoggerLogLevel": "Critical", //最大自身日志等级
      //"LogLevel": {  //日志等级过滤
      //  "Default": "Trace",
      //  "Microsoft": "Warning"
      //},
      "Targets": [ //目标
        {
          "Enable": true, //启用标识
          "Name": "ConsoleLog", //目标名称
          "Type": "Console", //控制台日志类型
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}", //日志格式
          "Colors": { //日志颜色
            "Trace": "DarkGray",
            "Debug": "DarkGreen",
            "Information": "Gray",
            "Warning": "DarkYellow",
            "Error": "DarkRed",
            "Critical": "Red"
          }
        },
        {
          "Enable": true, //启用标识
          "Name": "FileLog", //目标名称
          "Type": "File", //文件日志类型
          "Path": "${RootPath}\\Logs\\${Level}.log", //日志路径
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}" //日志格式
        }
      ],
      "Rules": [ //规则
        {
          "Enable": true, //启用标识
          "Name": "规则1", //规则名称
          "MinLevel": "Trace", //最小日志等级
          "MaxLevel": "Critical", //最大日志等级
          "Targets": [
            "ConsoleLog",
            "FileLog"
          ]
          //"LogLevel": { //日志等级过滤
          //  "Default": "Trace",
          //  "Microsoft": "Warning"
          //}
          //"Scopes": [ //作用域目标
          //  "PublicScope"
          //]
        }
      ]
    }
  }
}
