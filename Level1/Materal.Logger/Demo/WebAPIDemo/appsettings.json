{
  "AllowedHosts": "*",
  "MateralLogger": {
    "Application": "WebAPIDemo",
    "Targets": [
      {
        "Name": "LifeConsole",
        "Type": "Console",
        "Colors": {
          "Trace": "DarkGray",
          "Debug": "DarkGreen",
          "Information": "Gray",
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "Red"
        },
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
      }
    ],
    "Rules": [
      {
        "MinLevel": "Information",
        "MaxLevel": "Critical",
        "Targets": [
          "LifeConsole"
        ],
        "LogLevel": {
          "Default": "Information",
          "Microsoft.AspNetCore": "Warning"
        }
      }
    ]
  }
}