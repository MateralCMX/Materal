<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="appsettings.json" />
	</ItemGroup>
	<ItemGroup>
	  <Content Include="appsettings.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	    <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
	    <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
	  </Content>
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\Src\Materal.Logger.SqliteLog\Materal.Logger.SqliteLog.csproj" />
		<ProjectReference Include="..\..\Src\Materal.Logger.SqlServerLog\Materal.Logger.SqlServerLog.csproj" />
		<ProjectReference Include="..\..\Src\Materal.Logger.WebSocketLog\Materal.Logger.WebSocketLog.csproj" />
		<ProjectReference Include="..\..\Src\Materal.Logger\Materal.Logger.csproj" />
	</ItemGroup>
</Project>
