<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows10.0.19041.0</TargetFramework>
		<TargetPlatformMinVersion>10.0.17763.0</TargetPlatformMinVersion>
		<RootNamespace>WinUIDemo</RootNamespace>
		<Nullable>enable</Nullable>
		<ApplicationManifest>app.manifest</ApplicationManifest>
		<Platforms>x86;x64;ARM64</Platforms>
		<RuntimeIdentifiers>win10-x86;win10-x64;win10-arm64</RuntimeIdentifiers>
		<UseWinUI>true</UseWinUI>
		<!--不打包-->
		<WindowsPackageType>None</WindowsPackageType>
		<!--自包含 Windows App SDK Runtime-->
		<WindowsAppSDKSelfContained>true</WindowsAppSDKSelfContained>
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="appsettings.json" />
	</ItemGroup>
	<ItemGroup>
	  <Content Include="appsettings.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	    <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
	    <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
	  </Content>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="CommunityToolkit.Mvvm" Version="8.2.1" />
		<PackageReference Include="Microsoft.WindowsAppSDK" Version="1.2.230217.4" />
		<PackageReference Include="Microsoft.Windows.SDK.BuildTools" Version="10.0.22621.755" />
		<Manifest Include="$(ApplicationManifest)" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\..\Src\Materal.Logger.SqliteLog\Materal.Logger.SqliteLog.csproj" />
	  <ProjectReference Include="..\..\..\Src\Materal.Logger.SqlServerLog\Materal.Logger.SqlServerLog.csproj" />
	  <ProjectReference Include="..\..\..\Src\Materal.Logger.WebSocketLog\Materal.Logger.WebSocketLog.csproj" />
	  <ProjectReference Include="..\..\..\Src\Materal.Logger\Materal.Logger.csproj" />
	</ItemGroup>
</Project>
