{
  "MateralLogger": {
    "Targets": [
      {
        "Enable": true,
        "Name": "FileLog",
        "Type": "File",
        "Path": "${RootPath}\\Logs\\Log.log",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
      }
    ],
    "Rules": [
      {
        "Enable": true,
        "Name": "规则1",
        "Targets": [
          "FileLog"
        ],
        "MinLevel": "Trace",
        "MaxLevel": "Critical",
        "LogLevel": {
          "Default": "Trace",
          "Microsoft": "Warning"
        }
      }
    ]
  }
}