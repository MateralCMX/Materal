<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1;net6.0;net8.0</TargetFrameworks>
		<LangVersion>12.0</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IsPackable>true</IsPackable>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Version>1.1.76</Version>
		<Company>Materal</Company>
		<Product>Materal日志抽象</Product>
		<Authors>Materal</Authors>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Title>Materal-日志抽象</Title>
		<Description>包含日志抽象内容</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<NoWarn>1701;1702;SYSLIB1045;CA1816;</NoWarn>
	</PropertyGroup>
	<ItemGroup>
		<None Include="../../../../README.md" Pack="true" PackagePath="" />
		<None Include="../../../../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Materal.Utils" Version="1.1.76" />
		<PackageReference Include="Microsoft.Extensions.Logging.Configuration" Version="*" />
	</ItemGroup>
</Project>