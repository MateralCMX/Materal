<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>10.0</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<PackageOutputPath>../../../../nupkgs</PackageOutputPath>
		<Version>1.1.26</Version>
		<Company>Materal</Company>
		<Product>Materal日志Asp.NET模块</Product>
		<Authors>Materal</Authors>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Title>Materal-日志AspNetCore</Title>
		<Description>包含在Asp.NET中使用日志所需的所有内容</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="../../../../README.md" Pack="true" PackagePath="" />
		<None Include="../../../../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.Logger\Materal.Logger.csproj" />
	</ItemGroup>
	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>
</Project>
