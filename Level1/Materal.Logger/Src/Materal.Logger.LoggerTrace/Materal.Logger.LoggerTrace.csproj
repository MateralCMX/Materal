<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFrameworks>net6.0;net8.0;net472;net481</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>12.0</LangVersion>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<IsPackable>true</IsPackable>
		<PackAsTool>true</PackAsTool>
		<Version>1.1.76</Version>
		<Company>Materal</Company>
		<Product>Materal日志追踪器</Product>
		<Authors>Materal</Authors>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Title>Materal-日志追踪器</Title>
		<Description>包含日志所需的基本内容</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<ToolCommandName>MateralLoggerTrace</ToolCommandName>
	</PropertyGroup>
	<ItemGroup>
		<None Include="../../../../README.md" Pack="true" PackagePath="" />
		<None Include="../../../../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
		<Compile Include="../Materal.Logger.Abstractions/Log.cs" Link="Models\Log.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="*" />
		<PackageReference Include="System.CommandLine" Version="2.0.0-beta4.22272.1" />
		<PackageReference Include="Materal.Utils" Version="1.1.76" />
	</ItemGroup>
</Project>
