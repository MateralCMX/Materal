{
  //LogLevel:Trace,Debug,Information,Warning,Error,Critical,None
  //Format: DateTime,Date,Time,Year,Month,Day,Hour,Minute,Second,Level,Scope,Message,CategoryName,Exception,ProgressID,ThreadID,Application,MachineName
  //Path: Date,Year,Month,Day,Hour,Minute,Second,Level,Scope,CategoryName,Exception,ProgressID,ThreadID,Application,MachineName
  //Message:DateTime,Date,Time,Year,Month,Day,Hour,Minute,Second,Level,Scope,CategoryName,Exception,ProgressID,ThreadID,Application,MachineName
  "Logging": {
    "LogLevel": { //兼容默认日志配置,优先级1
      "Default": "Trace",
      "Microsoft": "Warning"
    }
  },
  "MateralLogger": {
    "Application": "${ApplicationName}", //应用程序名称
    "Mode": "Strict", //Normal,Strict=>Normal[普通模式,应用程序退出时不会等待日志记录完毕],Strict[严格模式,应用程序退出时会等待所有日志记录完成]，默认Strict
    "LoggerLogLevel": { //日志自身日志配置
      "MinLevel": "Trace",
      "MaxLevel": "Critical"
    },
    "LogLevel": { //默认日志配置,优先级2
      "Default": "Trace",
      "Microsoft": "Warning"
    },
    "BufferPushInterval": 1000,
    "BufferCount": 2000,
    "Targets": [ //目标组
      {
        "Enable": true, //启用标识
        "Name": "ConsoleLog", //目标名称
        "Type": "Console", //控制台日志类型
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}", //日志格式
        "Colors": { //日志颜色
          "Trace": "DarkGray",
          "Debug": "DarkGreen",
          "Information": "Gray",
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "Red"
        }
      },
      {
        "Enable": true,
        "Name": "FileLog",
        "Type": "File", //文件日志类型
        "Path": "${RootPath}\\Logs\\${Level}.log", //日志路径
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}" //日志格式
      },
      {
        "Enable": true,
        "Name": "HttpLog",
        "Type": "Http", //Http日志类型
        "Url": "http://localhost:5000/api/Log/Write${Level}", //Http请求地址
        "HttpMethod": "POST" //Http请求方法
      },
      {
        "Enable": true,
        "Name": "SqliteLog",
        "Type": "Sqlite", //Sqlite日志类型,数据库路径或链接字符串必选其一，优先使用数据库接字符串
        "Path": "${RootPath}\\Logs\\MateralLogger.db", //Sqlite数据库路径
        "ConnectionString": "Data Source=${RootPath}\\Logs\\MateralLogger.db", //Sqlite数据库连接字符串
        "TableName": "${Level}Log", //日志表名称，默认为MateralLogger
        "Fileds": [ //字段组，默认不配置时如下
          {
            "Name": "ID",
            "Type": "TEXT(36)",
            "Value": "${LogID}",
            "PK": true
          },
          {
            "Name": "CreateTime",
            "Type": "DATE",
            "Value": "${DateTime}",
            "Index": true,
            "IsNull": false
          },
          {
            "Name": "Level",
            "Type": "TEXT",
            "Value": "${Level}"
          },
          {
            "Name": "ProgressID",
            "Type": "TEXT",
            "Value": "${ProgressID}"
          },
          {
            "Name": "ThreadID",
            "Type": "TEXT",
            "Value": "${ThreadID}"
          },
          {
            "Name": "Scope",
            "Type": "TEXT",
            "Value": "${Scope}"
          },
          {
            "Name": "MachineName",
            "Type": "TEXT",
            "Value": "${MachineName}"
          },
          {
            "Name": "CategoryName",
            "Type": "TEXT",
            "Value": "${CategoryName}"
          },
          {
            "Name": "Application",
            "Type": "TEXT",
            "Value": "${Application}"
          },
          {
            "Name": "Message",
            "Type": "TEXT",
            "Value": "${Message}"
          },
          {
            "Name": "Exception",
            "Type": "TEXT",
            "Value": "${Exception}"
          }
        ]
      },
      {
        "Enable": true,
        "Name": "SqlServerLog",
        "Type": "SqlServer", //SqlServer日志类型
        "ConnectionString": "Data Source=82.156.11.176;Database=LogTestDB; User ID=sa; Password=gdb@admin678;MultipleActiveResultSets=True;Encrypt=True;TrustServerCertificate=True;", //SqlServer数据库连接字符串
        "TableName": "${Level}Log", //日志表名称，默认为MateralLogger
        "Fileds": [ //字段组，默认不配置时如下
          {
            "Name": "ID",
            "Type": "[uniqueidentifier]",
            "Value": "${LogID}",
            "PK": true
          },
          {
            "Name": "CreateTime",
            "Type": "[datetime2](7)",
            "Value": "${DateTime}",
            "Index": true,
            "IsNull": false
          },
          {
            "Name": "Level",
            "Type": "[nvarchar](50)",
            "Value": "${Level}"
          },
          {
            "Name": "ProgressID",
            "Type": "[nvarchar](20)",
            "Value": "${ProgressID}"
          },
          {
            "Name": "ThreadID",
            "Type": "[nvarchar](20)",
            "Value": "${ThreadID}"
          },
          {
            "Name": "Scope",
            "Type": "[nvarchar](100)",
            "Value": "${Scope}"
          },
          {
            "Name": "MachineName",
            "Type": "[nvarchar](100)",
            "Value": "${MachineName}"
          },
          {
            "Name": "CategoryName",
            "Type": "[nvarchar](100)",
            "Value": "${CategoryName}"
          },
          {
            "Name": "Application",
            "Type": "[nvarchar](50)",
            "Value": "${Application}"
          },
          {
            "Name": "Message",
            "Type": "[nvarchar](Max)",
            "Value": "${Message}"
          },
          {
            "Name": "Exception",
            "Type": "[nvarchar](Max)",
            "Value": "${Exception}"
          }
        ]
      },
      {
        "Enable": true,
        "Name": "WebSocketLog",
        "Type": "WebSocket", //WebSocket日志类型
        "Port": 5002 //WebSocket端口
      },
      {
        "Enable": true,
        "Name": "MongoLog",
        "Type": "Mogo", //Mongo日志类型
        "ConnectionString": "mongodb://localhost:27017/", //WebSocket端口
        "DBName": "MateralLogger", //数据库名称
        "CollectionName": "Logger${Year}${Month}${Day}" //集合名称
      }
    ],
    "Rules": [ //规则组
      {
        "Enable": true, //启用标识
        "Name": "配置文件全目标规则", //规则名称
        "Targets": [ //目标组
          //"ConsoleLog"
          "FileLog"
          //"HttpLog",
          //"SqliteLog",
          //"SqlServerLog",
          //"WebSocketLog"
        ],
        "MinLevel": "Trace", //最小日志等级
        "MaxLevel": "Critical", //最大日志等级
        "LogLevel": { //默认日志配置,优先级3
          "Default": "Trace",
          "Microsoft": "Warning"
        },
        "Scopes": [ //作用域组
          "CustomScope"
        ]
      }
    ]
  }
}