<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>12.0</LangVersion>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>1701;1702;CA2254</NoWarn>
		<IsPackable>true</IsPackable>
		<Version>1.1.75</Version>
		<Authors>Materal</Authors>
		<Product>Materal.EventBus</Product>
		<Description>Materal事件总线</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="../../../../README.md" Pack="true" PackagePath="" />
		<None Include="../../../../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="*" />
		<PackageReference Include="Polly" Version="*" />
		<PackageReference Include="RabbitMQ.Client" Version="*" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.EventBus.Abstraction\Materal.EventBus.Abstraction.csproj" />
	</ItemGroup>
</Project>
