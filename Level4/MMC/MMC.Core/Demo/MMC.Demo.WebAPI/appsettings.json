{
  "ApplicationName": "MMC.Demo",
  "Authorization": {
    "Audience": "WebAPI",
    "ExpiredTime": "18000",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig"
  },
  "Exception": {
    "ShowException": true,
    "ErrorMessage": "服务器出错了！请联系管理员。"
  },
  "Swagger": {
    "Enable": true,
    "Version": "v1",
    "Description": "提供WebAPI接口"
  },
  "DBConfig": {
    "Source": "D:\\Project\\Materal\\DataBase\\MergeBlock\\MMBDemo.db"
  },
  "Logging": {
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [
            "ConsoleLog"
          ]
        }
      ]
    }
  }
}
