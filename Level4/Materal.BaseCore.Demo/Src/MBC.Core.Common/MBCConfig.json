{
  "ConsulConfig": {
    "Enable": true,
    "Url": "http://127.0.0.1:8500",
    "HealthInterval": 30,
    "ReconnectionInterval": 30
  },
  "EnableAuthentication": true,
  "EventBusConfig": {
    "HostName": "127.0.0.1",
    "Port": 5672,
    "UserName": "admin",
    "Password": "123456",
    "ExchangeName": "MBCExchange_DEV",
    "RetryCount": 5
  },
  "Exception": {
    "ShowException": true,
    "ErrorMessage": "服务器发生错误,请联系后台工程师处理。"
  },
  "HttpClienUrltConfig": {
    "BaseUrl": "http://175.27.254.187:8700/",
    "Suffix": "API"
  },
  "JWT": {
    "Audience": "WebAPI",
    "ExpiredTime": "18000",
    "Issuer": "MateralReleaseCenter",
    "Key": "MateralCMXConfig"
  },
  "MateralLogger": {
    "Application": "${ApplicationName}",
    "Targets": [
      {
        "Name": "LifeConsole",
        "Type": "Console",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}"
      },
      {
        "Name": "ErrorConsole",
        "Type": "Console",
        "Colors": {
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "DarkRed"
        },
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
      },
      {
        "Name": "LevelFile",
        "Type": "File",
        "Path": "${RootPath}\\Logs\\${Date}\\${Level}.log",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
      }
    ],
    "Rules": [
      {
        "MinLevel": "Information",
        "MaxLevel": "Information",
        "Targets": [
          "LifeConsole",
          "LevelFile"
        ],
        "Ignores": [
          "Microsoft.AspNetCore.*",
          "Microsoft.EntityFrameworkCore.*"
        ]
      },
      {
        "MinLevel": "Warning",
        "MaxLevel": "Critical",
        "Targets": [
          "ErrorConsole",
          "LevelFile"
        ]
      }
    ]
  },
  "SwaggerConfig": {
    "Enable": true,
    "Title": null,
    "Version": null,
    "Description": null,
    "Author": null,
    "Email": null
  }
}