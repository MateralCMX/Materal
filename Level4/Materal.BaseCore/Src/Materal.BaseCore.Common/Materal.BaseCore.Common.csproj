<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net6.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageOutputPath>../../../../nupkgs</PackageOutputPath>
		<Version>1.1.65</Version>
		<Company>古典部工作室</Company>
		<Product>Materal.BaseCore</Product>
		<Authors>Materal</Authors>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Title>Materal.BaseCore.Common</Title>
		<Description>请引用对应名称包</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="../../../../README.md" Pack="true" PackagePath="" />
		<None Include="../../../../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="*" />
		<PackageReference Include="Materal.Utils" Version="1.1.65" />
		<PackageReference Include="Microsoft.Extensions.Logging" Version="*" />
		<PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="*" />
		<PackageReference Include="NetCore.AutoRegisterDi" Version="*" />
	</ItemGroup>
</Project>
