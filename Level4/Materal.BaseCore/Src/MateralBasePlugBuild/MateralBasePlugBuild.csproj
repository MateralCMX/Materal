<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFrameworks>net6.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>12.0</LangVersion>
		<SatelliteResourceLanguages>en</SatelliteResourceLanguages>
		<EnableDynamicLoading>true</EnableDynamicLoading>
		<IsPublish>true</IsPublish>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.5.0" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
		<PackageReference Include="System.Collections.Immutable" Version="6.*" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<PackageReference Include="System.Collections.Immutable" Version="*" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.BaseCore.CodeGenerator\Materal.BaseCore.CodeGenerator.csproj" />
	</ItemGroup>
	<ItemGroup>
		<None Update="ModelData.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
</Project>
