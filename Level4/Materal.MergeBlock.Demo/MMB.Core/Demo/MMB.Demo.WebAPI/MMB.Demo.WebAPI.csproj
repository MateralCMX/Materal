		<PackageReference Include="Materal.MergeBlock.Logger" Version="*" />
		<PackageReference Include="Materal.MergeBlock.Oscillator" Version="*" />
		<PackageReference Include="Materal.MergeBlock.Swagger" Version="*" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\MMB.Demo.Appllication\MMB.Demo.Appllication.csproj" />
	</ItemGroup>
