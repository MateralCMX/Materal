{
  "ConfigFrom": "File",
  "MateralLogger": {
    "Application": "${ApplicationName}",
    "Targets": [
      {
        "Name": "ConsoleLog",
        "Type": "Console",
        "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}",
        "Colors": {
          "Trace": "DarkGray",
          "Debug": "DarkGreen",
          "Information": "Gray",
          "Warning": "DarkYellow",
          "Error": "DarkRed",
          "Critical": "Red"
        }
      }
    ],
    "Rules": [
      {
        "Name": "配置文件全目标规则",
        "Targets": [
          "ConsoleLog"
        ],
        "MinLevel": "Trace",
        "MaxLevel": "Critical",
        "LogLevel": {
          "Default": "Trace",
          "Microsoft.AspNetCore": "Warning",
          "Microsoft.Extensions.Hosting": "Warning"
        }
      }
    ]
  },
  "Authorization": {
    "Audience": "WebAPI",
    "ExpiredTime": "18000",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig"
  },
  "Consul": {
    "Enable": true,
    "ServiceName": "MMB.Demo.WebAPI",
    "Tags": [],
    "ConsulUrl": {
      "Host": "127.0.0.1",
      "Port": 8500,
      "IsSSL": false
    },
    "ServiceUrl": {
      "Host": "localhost",
      "Port": 5000,
      "IsSSL": false
    },
    "HealthConfig": {
      "HealthInterval": 10,
      "ReconnectionInterval": 10,
      "HealthUrl": {
        "Host": "localhost",
        "Port": 5000,
        "IsSSL": false,
        "Path": "/api/Health"
      }
    }
  }
}
