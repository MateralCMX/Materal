{
  "AccessLog": {
    "MaxBodySize": 2048
  },
  "AllowedHosts": "*",
  "Authorization": {
    "Audience": "WebAPI",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig",
    "ExpiredTime": "18000"
  },
  "ApplicationName": "MMB.Demo",
  "BaseUrl": "http://127.0.0.1:5000",
  "Consul": {
    "Enable": true,
    "Tags": [ "Materal", "Materal.MergeBlock", "MMB" ],
    "ConsulUrl": {
      "Host": "127.0.0.1",
      "Port": 8500,
      "IsSSL": false
    },
    "ServiceUrl": {
      "Host": "localhost",
      "Port": 5000,
      "IsSSL": false
    },
    "HealthInterval": 10,
    "HealthUrl": {
      //"Host": "localhost",
      "Host": "host.docker.internal",
      "Port": 5000,
      "IsSSL": false,
      "Path": "/api/Health"
    },
    "ServiceNameSuffix": "_ProAPI"
  },
  "Exception": {
    "ShowException": false,
    "ErrorMessage": "服务器出错了！请联系管理员。"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "Scopes": {
        "MergeBlockAccessLog": "None"
      },
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
        },
        {
          "Name": "AccessLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [
            "ConsoleLog"
          ]
        },
        {
          "Name": "访问日志规则",
          "Targets": [
            "AccessLog"
          ],
          "Scopes": {
            "Default": "None",
            "MergeBlockAccessLog": "Trace"
          }
        }
      ]
    }
  },
  "Swagger": {
    "Enable": true,
    "Title": "MMB应用程序",
    "Version": "v1",
    "Description": "提供WebAPI接口",
    "EnableAuthentication": null
  },
  "ModulesDirectories": [
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\Modules\\Materal.MergeBlock.Logger\\bin\\Debug",
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\Modules\\Materal.MergeBlock.Swagger\\bin\\Debug",
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\Modules\\Materal.MergeBlock.ExceptionInterceptor\\bin\\Debug",
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\Modules\\Materal.MergeBlock.Authorization\\bin\\Debug",
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\Modules\\Materal.MergeBlock.AccessLog\\bin\\Debug",
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\Modules\\Materal.MergeBlock.Consul\\bin\\Debug",
    "D:\\Project\\Materal\\Materal\\Level4\\Materal.MergeBlock\\MMB\\Demo\\MMB.Demo.Application\\bin\\Debug"
  ]
}
