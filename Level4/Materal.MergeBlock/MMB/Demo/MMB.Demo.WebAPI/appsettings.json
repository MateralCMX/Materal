{
  "ApplicationName": "MMB.Demo",
  "Authorization": {
    "Audience": "WebAPI",
    "ExpiredTime": "18000",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig"
  },
  "Exception": {
    "ShowException": true,
    "ErrorMessage": "服务器出错了！请联系管理员。"
  },
  "Swagger": {
    "Enable": true,
    "Version": "v1",
    "Description": "提供WebAPI接口"
  },
  "Logging": {
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "Scopes": {
        "MergeBlockAccessLog": "None"
      },
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
        },
        {
          "Name": "AccessLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message} ${Response.StatusCode}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [
            "ConsoleLog"
          ]
        },
        {
          "Name": "访问日志规则",
          "Targets": [
            "AccessLog"
          ],
          "Scopes": {
            "Default": "None",
            "MergeBlockAccessLog": "Trace"
          }
        }
      ]
    }
  },
  "DBConfig": {
    "Source": "D:\\Project\\Materal\\DataBase\\MergeBlock\\MMBDemo.db"
  },
  "EventBus": {
    "HostName": "175.27.194.19",
    "Port": 5672,
    "UserName": "GDB",
    "Password": "GDB2022",
    "ExchangeName": "MMBEventBusExchange_Dev",
    "RetryNumber": 5,
    "RetryIntervalSecond": 5,
    "DiscardErrorMessage": false,
    "QueueName": "MMBDemo_Dev"
  },
  "Consul": {
    "Enable": false,
    "ServiceName": "MMBDemo_DevAPI",
    "Tags": [ "MMB", "MMBDemo" ],
    "ServiceUrl": {
      "Host": "localhost",
      "Port": 5000,
      "IsSSL": false
    },
    "HealthConfig": {
      "HealthUrl": {
        "Host": "host.docker.internal",
        "Port": 5000,
        "IsSSL": false,
        "Path": "/api/Health"
      }
    }
  }
}
