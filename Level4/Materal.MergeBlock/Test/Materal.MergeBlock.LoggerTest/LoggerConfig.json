{
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
      //"Microsoft.Extensions.Hosting": "Warning",
      //"Microsoft.AspNetCore": "Warning",
      //"Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None",
      //"Microsoft.EntityFrameworkCore": "Warning",
      //"Materal.MergeBlock.Oscillator": "Information",
      //"MergeBlock": "Warning"
    },
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "CustomData": {
        "ApplicationName": "MergeBlockApplication"
      },
      "Scopes": {
        "MergeBlockAccessLog": "None"
      },
      "Targets": [
        {
          "Name": "AccessLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}"
        },
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [
            "ConsoleLog"
          ]
        },
        {
          "Name": "访问日志规则",
          "Targets": [
            "AccessLog"
          ],
          "Scopes": {
            "Default": "None",
            "MergeBlockAccessLog": "Trace"
          }
        }
      ]
    }
  }
}
