<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net6.0;net8.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>12.0</LangVersion>
		<EnableDynamicLoading>true</EnableDynamicLoading>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IsPublish>true</IsPublish>
		<NoWarn>CA2254</NoWarn>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Materal.MergeBlock.Abstractions" Version="1.1.76" />
		<PackageReference Include="Materal.Utils.Consul" Version="1.1.76" />
		<PackageReference Include="MMLib.SwaggerForOcelot" Version="*" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.Gateway\Materal.Gateway.csproj" />
	</ItemGroup>
	<ItemGroup>
		<None Update="GatewayManagement\**">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
	<ItemGroup>
	  <None Update="Ocelot.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>
</Project>
