{
  "Routes": [
    {
      "UpstreamPathTemplate": "/MateralWebAPITest/api/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.1",
      "ServiceName": "MateralWebAPITest",
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "MateralWebAPITest",
      "Index": 6
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "127.0.0.1",
      "Port": 8500,
      "Type": "Consul",
      "IsSSL": false
    }
  },
  "SwaggerEndPoints": [
    {
      "Key": "MateralWebAPITest",
      "TakeServersFromDownstreamService": true,
      "Config": [
        {
          "Name": "MateralWebAPITest",
          "Version": "v1",
          "Service": {
            "Name": "MateralWebAPITest",
            "Path": "/swagger/v1/swagger.json"
          }
        }
      ]
    }
  ]
}
