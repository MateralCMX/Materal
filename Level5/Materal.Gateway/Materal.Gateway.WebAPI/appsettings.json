{
  "AccessLog": {
    "MaxBodySize": 2048
  },
  "AllowedHosts": "*",
  "Authorization": {
    "Audience": "WebAPI",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig",
    "ExpiredTime": "18000"
  },
  "ApplicationName": "Materal.Gateway",
  "BaseUrl": "http://127.0.0.1:5000",
  "Exception": {
    "ShowException": false,
    "ErrorMessage": "服务器出错了！请联系管理员。"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "Scopes": {
        "MergeBlockAccessLog": "None"
      },
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
        },
        {
          "Name": "AccessLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [
            "ConsoleLog"
          ]
        },
        {
          "Name": "访问日志规则",
          "Targets": [
            "AccessLog"
          ],
          "Scopes": {
            "Default": "None",
            "MergeBlockAccessLog": "Trace"
          }
        }
      ]
    }
  },
  "ModulesDirectories": [],
  "Users": [
    {
      "Account": "Admin",
      "Password": "123456"
    }
  ]
}