<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<LangVersion>12.0</LangVersion>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IsPackable>true</IsPackable>
		<Title>Materal网关</Title>
		<Company>Materal</Company>
		<Product>Materal</Product>
		<Authors>Materal</Authors>
		<Version>1.1.76</Version>
		<Description>Materal网关</Description>
		<PackageReleaseNotes>https://github.com/MateralCMX/Materal</PackageReleaseNotes>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<NoWarn>1701;1702;CA1512</NoWarn>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Materal.Utils.Consul" Version="1.1.76" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
		<PackageReference Include="Ocelot" Version="18.*" />
		<PackageReference Include="Ocelot.Cache.CacheManager" Version="18.*" />
		<PackageReference Include="Ocelot.Provider.Consul" Version="18.*" />
		<PackageReference Include="Ocelot.Provider.Polly" Version="18.*" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<PackageReference Include="Ocelot" Version="*" />
		<PackageReference Include="Ocelot.Cache.CacheManager" Version="*" />
		<PackageReference Include="Ocelot.Provider.Consul" Version="*" />
		<PackageReference Include="Ocelot.Provider.Polly" Version="*" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.Gateway.Abstractions\Materal.Gateway.Abstractions.csproj" />
	</ItemGroup>
	<ItemGroup>
		<None Include="../../../README.md" Pack="true" PackagePath="" />
		<None Include="../../../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>
</Project>
