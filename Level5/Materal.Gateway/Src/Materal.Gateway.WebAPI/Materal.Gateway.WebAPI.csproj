<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<LangVersion>12.0</LangVersion>
		<IsPublish>true</IsPublish>
	</PropertyGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.*" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<PackageReference Include="Materal.MergeBlock">
		  <Version>1.1.74</Version>
		</PackageReference>
		<PackageReference Include="Materal.MergeBlock.Authorization">
		  <Version>1.1.74</Version>
		</PackageReference>
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="Materal.MergeBlock.ExceptionInterceptor" Version="1.1.74" />
	  <PackageReference Include="Materal.MergeBlock.Logger" Version="1.1.74" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\Materal.Gateway.Application\Materal.Gateway.Application.csproj" />
	</ItemGroup>
</Project>
