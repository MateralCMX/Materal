<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<InvariantGlobalization>true</InvariantGlobalization>
		<LangVersion>12.0</LangVersion>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IsPublish>true</IsPublish>
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="Properties\launchSettings.json" />
	</ItemGroup>
	<ItemGroup>
	  <Content Include="Properties\launchSettings.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	    <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
	    <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
	  </Content>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="8.*">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Materal.MergeBlock" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.Logger" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.Swagger" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.Authorization" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.AccessLog" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.ExceptionInterceptor" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.Consul" Version="1.1.74" />
		<PackageReference Include="Materal.MergeBlock.EventBus" Version="1.1.74" />
		<!--<PackageReference Include="Materal.MergeBlock.Oscillator" Version="*" />-->
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\RC.ServerCenter.Application\RC.ServerCenter.Application.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Content Update="appsetting.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
</Project>
