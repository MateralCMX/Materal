<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">
	<PropertyGroup>
		<TargetFrameworks>net6.0;net8.0</TargetFrameworks>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<LangVersion>10.0</LangVersion>
		<IsPublish>true</IsPublish>
		<Version>1.1.66</Version>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="AntDesign" Version="0.16.2" />
		<PackageReference Include="Blazored.LocalStorage" Version="4.4.0" />
		<PackageReference Include="Blazored.SessionStorage" Version="2.4.0" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
		<PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="6.*" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="6.*" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="6.*" PrivateAssets="all" />
		<PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="6.*" />
	</ItemGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<PackageReference Include="Microsoft.AspNetCore.Components.Authorization" Version="8.*" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.*" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="8.*" PrivateAssets="all" />
		<PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="8.*" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\RC.Authority\RC.Authority.HttpClient\RC.Authority.HttpClient.csproj" />
		<ProjectReference Include="..\..\RC.Deploy\RC.Deploy.HttpClient\RC.Deploy.HttpClient.csproj" />
		<ProjectReference Include="..\..\RC.EnvironmentServer\RC.EnvironmentServer.HttpClient\RC.EnvironmentServer.HttpClient.csproj" />
		<ProjectReference Include="..\RC.ServerCenter.HttpClient\RC.ServerCenter.HttpClient.csproj" />
	</ItemGroup>
</Project>
