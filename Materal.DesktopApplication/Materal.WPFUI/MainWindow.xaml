<Window x:Class="Materal.WPFUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpfCustomControlLib="clr-namespace:Materal.WPFCustomControlLib;assembly=Materal.WPFCustomControlLib"
        xmlns:dateTimePicker="clr-namespace:Materal.WPFUserControlLib.DateTimePicker;assembly=Materal.WPFUserControlLib"
        xmlns:dateTimePicker1="clr-namespace:Materal.WPFCustomControlLib.DateTimePicker;assembly=Materal.WPFCustomControlLib"
        mc:Ignorable="d"
        Title="MainWindow" Loaded="Window_Loaded" Height="450" Width="800" d:DataContext="{d:DesignData Type=MainWindow }">
    <Window.Resources>
    </Window.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Grid.Column="0">
            <dateTimePicker1:DateTimePicker x:Name="qwer" Value="2018-01-01 01:02:03" Format="yyyyMMddHHmmss" VerticalAlignment="Center"></dateTimePicker1:DateTimePicker>
        </Grid>
        <Grid Grid.Row="0" Grid.Column="1">
            <Button Click="Button_Click">aaaaaaa</Button>
        </Grid>
        <Grid Grid.Row="1" Grid.Column="0">
        </Grid>
        <Grid Grid.Row="1" Grid.Column="1">
            <TextBox VerticalAlignment="Center" Text="{Binding StringValue}"></TextBox>
        </Grid>
    </Grid>
</Window>
