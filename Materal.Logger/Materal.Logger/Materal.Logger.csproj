<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageOutputPath>../nupkgs</PackageOutputPath>
		<Version>0.0.2</Version>
		<Company>Materal</Company>
		<Product>Materal日志模块</Product>
		<Authors>Materal</Authors>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Title>Materal-日志</Title>
		<Description>包含日志所需的所有内容</Description>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Materal.ConfigurationHelper" Version="1.1.19" />
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
		<PackageReference Include="Fleck" Version="1.2.0" />
		<PackageReference Include="System.Data.SqlClient" Version="4.8.5" />
		<PackageReference Include="System.Data.SQLite.Core" Version="1.0.117" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\Materal.Logger.Message\Materal.Logger.Message.csproj" />
	</ItemGroup>
</Project>
