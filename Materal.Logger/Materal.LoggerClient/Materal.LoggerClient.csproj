<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageOutputPath>./nupkg</PackageOutputPath>
		<Version>0.0.3</Version>
		<PackAsTool>true</PackAsTool>
		<ToolCommandName>Materal.Logger</ToolCommandName>
		<Company>Materal</Company>
		<Product>Materal日志模块</Product>
		<Authors>Materal</Authors>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Title>Materal-日志</Title>
		<Description>日志监听工具</Description>
	</PropertyGroup>
	<ItemGroup>
	  <PackageReference Include="Materal.ConvertHelper" Version="1.1.19" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\Materal.Logger.Message\Materal.Logger.Message.csproj" />
	</ItemGroup>
</Project>
