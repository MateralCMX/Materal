{
  "ReRoutes": [
    { //UserWebAPI  用户WebAPI
      "DownstreamPathTemplate": "/api/{everything}", //下游路径模板
      "DownstreamScheme": "http", //下游
      "DownstreamHostAndPorts": [ //下游主机和端口
        {
          "Host": "192.168.0.125",
          "Port": 8902
        }
      ],
      "UpstreamPathTemplate": "/UserAPI/{everything}", //上游路径模板
      "UpstreamHttpMethod": [ "Get", "Post" ], //上游HttpMethod
      "DangerousAcceptAnyServerCertificateValidator": true, //忽略SSL错误
      "ReRouteIsCaseSensitive": true //大小写敏感
    },
    { //UploadFileAPI  文件上传WebAPI
      "DownstreamPathTemplate": "/api/{everything}", //下游路径模板
