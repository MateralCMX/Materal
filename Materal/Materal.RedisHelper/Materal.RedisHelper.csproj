	<ItemGroup>
		<PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
		<PackageReference Include="StackExchange.Redis" Version="2.6.90" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.ConvertHelper\Materal.ConvertHelper.csproj" />
		<ProjectReference Include="..\Materal.DateTimeHelper\Materal.DateTimeHelper.csproj" />
	</ItemGroup>
