{
  "Routes": [
    {
      "UpstreamPathTemplate": "/GDBAuthority_Dev/api/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ],
      "DownstreamPathTemplate": "/AuthorityAPI/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.1",
      "ServiceName": "GDBAuthority_Dev",
      "LoadBalancerOptions": {
        "Type": "NoLoadBalancer"
      },
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "GDBAuthorityAPI_Dev"
    },
    {
      "UpstreamPathTemplate": "/GDBSingleMerchantMall_Dev/api/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ],
      "DownstreamPathTemplate": "/SingleMerchantMallAPI/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.1",
      "ServiceName": "GDBSingleMerchantMall_Dev",
      "LoadBalancerOptions": {
        "Type": "NoLoadBalancer"
      },
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "GDBSingleMerchantMall_Dev"
    },
    {
      "UpstreamPathTemplate": "/GDBLogger_Dev/api/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ],
      "DownstreamPathTemplate": "/GDBLoggerAPI/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHttpVersion": "1.1",
      "ServiceName": "GDBLogger_Dev",
      "LoadBalancerOptions": {
        "Type": "NoLoadBalancer"
      },
      "DangerousAcceptAnyServerCertificateValidator": true,
      "SwaggerKey": "GDBLogger_Dev"
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "127.0.0.1",
      "Port": 8500,
      "Type": "Consul",
      "IsSSL": false
    }
  },
  "SwaggerEndPoints": [
    {
      "Key": "GDBAuthority_Dev",
      "TakeServersFromDownstreamService": true,
      "Config": [
        {
          "Name": "GDBAuthority_Dev",
          "Version": "latest",
          "Service": {
            "Name": "GDBAuthority_Dev",
            "Path": "/swagger/GDB.Authority/swagger.json"
          }
        }
      ]
    },
    {
      "Key": "GDBSingleMerchantMall_Dev",
      "TakeServersFromDownstreamService": true,
      "Config": [
        {
          "Name": "GDBSingleMerchantMall_Dev",
          "Version": "latest",
          "Service": {
            "Name": "GDBSingleMerchantMall_Dev",
            "Path": "/swagger/GDB.SingleMerchantMall/swagger.json"
          }
        }
      ]
    },
    {
      "Key": "GDBLogger_Dev",
      "TakeServersFromDownstreamService": true,
      "Config": [
        {
          "Name": "GDBLogger_Dev",
          "Version": "latest",
          "Service": {
            "Name": "GDBLogger_Dev",
            "Path": "/swagger/GDB.Logger/swagger.json"
          }
        }
      ]
    }
  ]
}