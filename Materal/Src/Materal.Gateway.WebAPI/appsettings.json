{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MergeBlock": "Information"
    },
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "MinLevel": "Trace",
      "MaxLevel": "Critical",
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [ "ConsoleLog" ]
        }
      ]
    }
  },
  "Authorization": {
    "Audience": "WebAPI",
    "ExpiredTime": "18000",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig"
  },
  "MergeBlock": {
    "ApplicationName": "Materal.Gateway.WebAPI",
    "LoadFromRootPath": true,
    "LoadFromPlugins": true,
    "Plugins": []
  },
  "Web": {
    "BaseUrl": "http://localhost:5001",
    "HttpBodyMaxSize": 10485760,
    "UsePascalCase": true
  },
  "Gateway": {
    "Users": [
      {
        "Account": "Admin",
        "Password": "123456"
      }
    ]
  },
  "AllowedHosts": "*"
}
