{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MergeBlock": "Information"
    },
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "MinLevel": "Trace",
      "MaxLevel": "Critical",
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}|[${MachineName},${ProgressID},${ThreadID}]\r\n${Message}\r\n${Exception}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [ "ConsoleLog" ]
        }
      ]
    }
  },
  "EventBus": {
    "HostName": "127.0.0.1",
    "Port": 5672,
    "UserName": "admin",
    "Password": "123456",
    "ExchangeName": "MergeBlockEventBusExchange",
    "QueueName": "MergeBlockEventBusQueue",
    "NameSuffix": "Dev",
    "RetryInterval": 5,
    "ErrorMeesageDiscard": false
  },
  "Authorization": {
    "Audience": "WebAPI",
    "ExpiredTime": "18000",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig"
  },
  "Consul": {
    "Enable": true,
    "Tags": [ "Materal", "Materal.MergeBlock", "Materal.MergeBlock.ModuleTest" ],
    "ConsulUrl": {
      "Host": "127.0.0.1",
      "Port": 8500,
      "IsSSL": false
    },
    "ServiceUrl": {
      "Host": "localhost",
      "Port": 5000,
      "IsSSL": false
    },
    "HealthInterval": 10,
    "HealthUrl": {
      "Host": "host.docker.internal",
      "Port": 5000,
      "IsSSL": false,
      "Path": "/api/Health"
    },
    "ServiceNameSuffix": "_TestAPI"
  },
  "MergeBlock": {
    "ApplicationName": "MMB_Demo_WebAPI",
    "LoadFromRootPath": true,
    "LoadFromPlugins": true,
    "Plugins": [
      {
        "Name": "测试插件",
        "RootPath": "E:\\Project\\Materal\\Materal\\Materal\\Test\\Materal.MergeBlock.ModuleTest\\bin\\Debug\\net8.0",
        "PluginType": [ 1, 2 ],
        "StartModule": null,
        "IsCollectible": true
      }
    ]
  },
  "Web": {
    "BaseUrl": "http://localhost:5001",
    "HttpBodyMaxSize": 10485760,
    "UsePascalCase": true
  },
  "AllowedHosts": "*"
}
