<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net6.0-windows;net472</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>10.0</LangVersion>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Title>Materal Windows工具包</Title>
		<Company>Materal</Company>
		<Product>Materal</Product>
		<Authors>Materal</Authors>
		<Version>0.0.9</Version>
		<Description>Materal Windows工具包</Description>
		<PackageReleaseNotes>https://github.com/MateralCMX/Materal</PackageReleaseNotes>
		<PackageOutputPath>../../nupkgs</PackageOutputPath>
	</PropertyGroup>
	<ItemGroup>
		<ProjectReference Include="..\Materal.Utils\Materal.Utils.csproj" />
	</ItemGroup>
</Project>
