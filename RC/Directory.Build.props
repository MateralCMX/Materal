<Project>
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<LangVersion>12.0</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
		<EnableDynamicLoading>true</EnableDynamicLoading>
		<ProduceReferenceAssembly>True</ProduceReferenceAssembly>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);CA1822;CA2254;NU1507;NU5104;WMC1006;</NoWarn>
	</PropertyGroup>
	<ItemGroup>
	  <PackageReference Include="Materal.Abstractions" />
	  <PackageReference Include="Materal.Extensions" />
	  <PackageReference Include="Materal.Extensions.DependencyInjection" />
	  <PackageReference Include="Materal.Utils" />
	  <PackageReference Include="Materal.MergeBlock.Abstractions" />
	</ItemGroup>
</Project>
