{
  "AccessLog": {
    "MaxBodySize": 2048
  },
  "AllowedHosts": "*",
  "Authorization": {
    "Audience": "WebAPI",
    "Issuer": "Materal.APP",
    "Key": "MateralCMXConfig",
    "ExpiredTime": "18000"
  },
  "Consul": {
    "Enable": true,
    "Tags": [ "Materal", "Materal.MergeBlock", "RC" ],
    "ConsulUrl": {
      "Host": "127.0.0.1",
      "Port": 8500,
      "IsSSL": false
    },
    "ServiceUrl": {
      "Host": "localhost",
      "Port": 5000,
      "IsSSL": false
    },
    "HealthInterval": 10,
    "HealthUrl": {
      //"Host": "localhost",
      "Host": "host.docker.internal",
      "Port": 5000,
      "IsSSL": false,
      "Path": "/api/Health"
    },
    "ServiceNameSuffix": "_ProAPI"
  },
  "EventBus": {
    "HostName": "127.0.0.1",
    "Port": 5672,
    "UserName": "admin",
    "Password": "123456",
    "ExchangeName": "RCEventBusExchange",
    "QueueName": "RCEventBusQueue",
    "NameSuffix": "",
    "RetryInterval": 5,
    "ErrorMeesageDiscard": false
  },
  "Exception": {
    "ShowException": false,
    "ErrorMessage": "服务器出错了！请联系管理员。"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "MateralLogger": {
      "Application": "${ApplicationName}",
      "Scopes": {
        "MergeBlockAccessLog": "None"
      },
      "Targets": [
        {
          "Name": "ConsoleLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}\r\n${Exception}"
        },
        {
          "Name": "AccessLog",
          "Type": "Console",
          "Format": "${DateTime}|${Application}|${Level}|${Scope}|${CategoryName}\r\n${Message}"
        }
      ],
      "Rules": [
        {
          "Name": "配置文件规则",
          "Targets": [
            "ConsoleLog"
          ]
        },
        {
          "Name": "访问日志规则",
          "Targets": [
            "AccessLog"
          ],
          "Scopes": {
            "Default": "None",
            "MergeBlockAccessLog": "Trace"
          }
        }
      ]
    }
  },
  "MergeBlock": {
    "ApplicationName": "RC",
    "LoadFromRootPath": true,
    "LoadFromPlugins": true,
    "Plugins": []
  },
  "Swagger": {
    "Enable": true,
    "Title": "RC应用程序",
    "Version": "v1",
    "Description": "提供WebAPI接口",
    "EnableAuthentication": null
  },
  "Web": {
    "BaseUrl": "http://localhost:5001",
    "HttpBodyMaxSize": 10485760,
    "UsePascalCase": true
  }
}